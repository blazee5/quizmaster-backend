[Unit]
Description=Quizmaster Backend
After=network.target

[Service]
User=root
WorkingDirectory=/root/apps/quizmaster-backend
ExecStart=/root/apps/quizmaster-backend/quizmaster-backend --config=/root/apps/grpc-auth/config/prod.yaml
Restart=always
RestartSec=4
StandardOutput=inherit

[Install]
WantedBy=multi-user.target